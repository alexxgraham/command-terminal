{
	"compilerOptions": {
		"target": "es2016",
		"jsx": "react-jsx",
		"module": "commonjs",
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		},
		"skipLibCheck": true
	}
}
